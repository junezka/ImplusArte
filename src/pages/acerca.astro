    <img  class="img" src={bgImages6} alt="">
    <img  class="img" src={bgImages7} alt="">
    <img  class="img" src={bgImages8} alt="">
    <img  class="img" src={bgImages9} alt="">
    <img  class="img" src={bgImages10} alt="">
    <img  class="img" src={bgImages11} alt="">
    <img  class="img" src={bgImages12} alt="">
    </div>
   </div>
    <style>
        .img { 
        border-radius: 10px;
        box-shadow: 0 5px 5px 0, 0 5px 5px 0;
        width: 1000px;
        margin: 15px;
        }
    </style>
</Layouts>