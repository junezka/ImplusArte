        <div class="snap-y snap-mandatory snap-center">
            <img class="img" src={bgImages11} alt="" />
        </div>
        <div class="snap-y snap-mandatory snap-center">
            <img class="img" src={bgImages12} alt="" />
        </div>
    </section>
    <style>
        .img {
            width: screen;
        }
    </style>
</Layouts>
