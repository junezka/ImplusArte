        <div class="snap-y snap-mandatory snap-center">
            <img class="img" src={bgImages} alt="" />
        </div>
        <div class="snap-y snap-mandatory snap-center">
            <img class="img" src={bgImages2} alt="" />
        </div>
        <div class="snap-y snap-mandatory snap-center">
            <img class="img" src={bgImages3} alt="" />
        </div>
        <div class="snap-y snap-mandatory snap-center">
            <img class="img" src={bgImages4} alt="" />
        </div>
        <div class="snap-y snap-mandatory snap-center">
            <img class="img" src={bgImages5} alt="" />
        </div>
        <div class="snap-y snap-mandatory snap-center">
            <img class="img" src={bgImages6} alt="" />
        </div>
        <div class="snap-y snap-mandatory snap-center">
            <img class="img" src={bgImages7} alt="" />
        </div>
        <div class="snap-y snap-mandatory snap-center">
            <img class="img" src={bgImages8} alt="" />
        </div>
        <div class="snap-y snap-mandatory snap-center">
            <img class="img" src={bgImages9} alt="" />
