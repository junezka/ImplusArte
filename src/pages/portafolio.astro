---
import Layouts from "../layouts/layouts.astro";
import bgImages from "../../public/images/1.jpg";
import bgImages2 from "../../public/images/2.jpg";
import bgImages3 from "../../public/images/3.jpg";
import bgImages4 from "../../public/images/4.jpg";
import bgImages5 from "../../public/images/5.jpg";
import bgImages6 from "../../public/images/6.jpg";
import bgImages7 from "../../public/images/7.jpg";
import bgImages8 from "../../public/images/8.jpg";
import bgImages9 from "../../public/images/9.jpg";
import bgImages10 from "../../public/images/10.jpg";
import bgImages11 from "../../public/images/11.jpg";
